# Database Configuration
ConnectionStrings__DefaultConnection=Host=localhost;Port=5432;Database=ascenddev;Username=postgres;Password=your_password
ConnectionStrings__Postgres=Host=localhost;Port=5432;Database=ascenddev;Username=postgres;Password=your_password
ConnectionStrings__RedisConnection=localhost:6379

# Redis Configuration
Redis__Host=localhost
Redis__Port=6379
Redis__User=default
Redis__Password=your_redis_password

# JWT Configuration
JwtSettings__SecretKey=your-super-secret-jwt-key-that-should-be-at-least-32-characters-long
JwtSettings__Key=your-super-secret-jwt-key-here-at-least-32-chars-long
JwtSettings__Issuer=AscendDev
JwtSettings__Audience=AscendDev-Users
JwtSettings__Audience__Dev=AscendDevAudience

# Email Configuration
Email__SmtpHost=smtp.gmail.com
Email__SmtpPort=587
Email__SmtpUsername=your_email@gmail.com
Email__SmtpPassword=your_app_password
Email__FromEmail=your_email@gmail.com
Email__FromName=AscendDev Platform

# OAuth - GitHub
OAuth__GitHub__ClientId=your-github-client-id
OAuth__GitHub__ClientSecret=your-github-client-secret

# OAuth - Google
OAuth__Google__ClientId=your-google-client-id
OAuth__Google__ClientSecret=your-google-client-secret

# Frontend Configuration
Frontend__BaseUrl=http://localhost:3000

# ASP.NET Core Environment
ASPNETCORE_ENVIRONMENT=Development

# Docker Compose Configuration
POSTGRES_CONTAINER_NAME=ascenddev-postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=ascenddev
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

REDIS_CONTAINER_NAME=ascenddev-redis
REDIS_PASSWORD=redis

API_CONTAINER_NAME=ascenddev-api
API_IMAGE=
API_PORT=5171
ASPNETCORE_URLS=http://+:5171

FRONTEND_CONTAINER_NAME=ascenddev-frontend
FRONTEND_PORT=3000
VITE_API_URL=http://localhost:5171/api

UPLOAD_CONFIG_CONTAINER_NAME=upload-config-files

# New Relic (Optional - for production monitoring)
NEW_RELIC_LICENSE_KEY=
NEW_RELIC_APP_NAME=AscendDev