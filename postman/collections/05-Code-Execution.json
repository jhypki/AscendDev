{
    "info": {
        "_postman_id": "code-execution-collection",
        "name": "05 - Code Execution",
        "description": "Code execution endpoints for running code in various languages",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Run Python Code",
            "event": [
                {
                    "listen": "test",
                    "script": {
                        "exec": [
                            "pm.test(\"Status code is 200\", function () {",
                            "    pm.response.to.have.status(200);",
                            "});",
                            "",
                            "pm.test(\"Response has execution result\", function () {",
                            "    const jsonData = pm.response.json();",
                            "    pm.expect(jsonData).to.have.property('output');",
                            "    pm.expect(jsonData).to.have.property('isSuccess');",
                            "    ",
                            "    console.log('Python execution result:');",
                            "    console.log('Success:', jsonData.isSuccess);",
                            "    console.log('Output:', jsonData.output);",
                            "    if (jsonData.error) {",
                            "        console.log('Error:', jsonData.error);",
                            "    }",
                            "});"
                        ],
                        "type": "text/javascript"
                    }
                }
            ],
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    },
                    {
                        "key": "Authorization",
                        "value": "Bearer {{accessToken}}"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n    \"Language\": \"python\",\n    \"Code\": \"print('Hello, World!')\\nprint('Python is working!')\\nresult = 2 + 2\\nprint(f'2 + 2 = {result}')\"\n}"
                },
                "url": {
                    "raw": "{{baseUrl}}/api/codeexecution/run",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "api",
                        "codeexecution",
                        "run"
                    ]
                },
                "description": "Execute Python code. Requires authentication."
            },
            "response": []
        },
